// Test 1: Mixed edges - Backward direction
// A -> B (directed, one-way)
// B <> C (undirected, two-way)

let mixed = network([
    A -> B: {weight: 5},
    B <> C: {weight: 3}
])

// Backward: C to A
// C->B works (undirected allows both directions)
// But B->A does NOT exist (A->B is one-way only)
// Expected: found = false, no path
dijkstra(mixed, "C", "A")
