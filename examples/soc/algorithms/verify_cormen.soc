// Verification of Cormen classic example
let cormen = network([
    s -> t: {weight: 10},
    s -> y: {weight: 5},
    t -> x: {weight: 1},
    t -> y: {weight: 2},
    y -> t: {weight: 3},
    y -> x: {weight: 9},
    y -> z: {weight: 2},
    x -> z: {weight: 4},
    z -> x: {weight: 6},
    z -> s: {weight: 7}
])

dijkstra(cormen, "s", "t")
dijkstra(cormen, "s", "x")
dijkstra(cormen, "s", "y")
dijkstra(cormen, "s", "z")
