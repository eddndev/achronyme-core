// Test 3: Triangle with mixed edges - Reverse
// Two undirected edges, one directed edge

let triangle = network([
    A <> B: {weight: 1},    // Two-way
    B <> C: {weight: 1},    // Two-way
    C -> A: {weight: 1}     // One-way only (C to A)
])

// From C to A: Direct edge C->A
// Expected: C->A, distance 1
dijkstra(triangle, "C", "A")
