// Statistics utilities module for testing

import { mean, std } from "stats"

// Custom statistics functions
let coefficientOfVariation = data => do {
    let m = mean(data);
    let s = std(data);
    (s / m) * 100
}

let range = data => do {
    let minimum = min(data);
    let maximum = max(data);
    maximum - minimum
}

export { coefficientOfVariation, range }
